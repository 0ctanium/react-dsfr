{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es5",
        "lib": ["ES2021", "DOM"],
        "esModuleInterop": true,
        "declaration": true,
        "outDir": "../../dist_test",
        "sourceMap": true,
        "newLine": "LF",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "incremental": true,
        "strict": true,
        "downlevelIteration": true,
        "jsx": "react",
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true
    },
    "include": ["../src"],
    "exclude": ["./frameworks", "./runtime"]
}
